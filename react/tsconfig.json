{
	"compilerOptions": {
		"target": "ES2020",
		"lib": [
			"DOM",
			"ES2020"
		],
		"types": [],
		"esModuleInterop": true,
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"allowJs": true,
		"strict": true,
		"baseUrl": "./",
		"jsx": "react-jsx",
		"noEmit": false,
		"declaration": true,
		"outDir": "./out/",
		"composite": true
	},
	"include": [
		"src"
	]
}